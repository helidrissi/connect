	:kP=\EJ:kN=\EK:al=\EE:cd=\EY:ce=\ET:cl=\E+:\
	:cm=\Ea%i%dR%dC:dl=\ER:k0=^AI\r:\
	:k1=^A@\r:k2=^AA\r:k3=^AB\r:k4=^AC\r:k5=^AD\r:k6=^AE\r:\
	:k7=^AF\r:k8=^AG\r:k9=^AH\r:\
	:K1=^AJ\r:K2=^AK\r:K3=^AL\r:K4=^AM\r:K5=^AN\r:K6=^AO\r:\
