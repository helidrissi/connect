# procedure pour dezipper les programmes transferes par ADSL chez les clients
#
cd /acuf/cdrom
/usr/local/bin/sudo -u root gunzip -f 2031.gz
/usr/local/bin/sudo -u root gunzip -f 2065.gz
/usr/local/bin/sudo -u root gunzip -f 2050.gz
/usr/local/bin/sudo -u root gunzip -f 2033.gz
/usr/local/bin/sudo -u root gunzip -f base.gz
/usr/local/bin/sudo -u root chown mistral:misinf *
/usr/local/bin/sudo -u root chmod 777 *
DATE=`date +%y%m%d`                                                  # JJHHMM
echo 20$DATE >INSTALL-CERFA
